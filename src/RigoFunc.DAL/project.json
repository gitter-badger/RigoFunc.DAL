{
  "version": "1.0.0-*",
  "description": "RigoFunc's data access library that inner use repository and unit of work patterns.",
  "authors": [ "RigoFunc" ],
  "packOptions": {
    "projectUrl": "https://github.com/xyting/RigoFunc.DAL",
    "licenseUrl": "",
    "repository": {
      "type": "git",
      "url": "https://github.com/xyting/RigoFunc.DAL.git"
    },
    "tags": [
      "repository",
      "unit of work",
      "uow",
      "UoW"
    ]
  },
  "buildOptions": {
    "optimize": true,
    "xmlDoc": true,
    "warningsAsErrors": true
  },
  "dependencies": {
    "NETStandard.Library": "1.5.0-rc2-24027",
    "Newtonsoft.Json": "8.0.3",
    "Microsoft.EntityFrameworkCore": "1.0.0-rc2-final",
    "Microsoft.EntityFrameworkCore.Relational": "1.0.0-rc2-final"
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "dnxcore50",
        "portable-net452+win81"
      ]
    }
  }
